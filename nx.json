{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "swell-devtest",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "none",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "none"
			},
			"library": {
				"style": "none",
				"linter": "eslint"
			}
		}
	},
	"defaultProject": "reviews-client"
}
